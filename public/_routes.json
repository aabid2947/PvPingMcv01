  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/blog",
      "dest": "/index.html"
    },
    {
      "src": "/blog/:id",
      "dest": "/index.html"
    },
    {
      "src": "/store",
      "dest": "/index.html"
    },
    {
      "src": "/vote",
      "dest": "/index.html"
    },
    {
      "src": "/rules",
      "dest": "/index.html"
    },
    {
      "src": "/pvpingmc",
      "dest": "/index.html"
    },
    {
      "src": "/originpass",
      "dest": "/index.html"
    },
    {
      "src": "/admin",
      "dest": "/admin/index.html"
    },
    {
      "src": "/admin/*",
      "dest": "/admin/index.html"
    },
    {
